﻿@model List<BooklyProject.Entities.PhotoGallery>


<section id="instagram">
    <div class="container">
        <div class="text-center mb-4">
            <h3>Fotoğraf Galerisi</h3>
        </div>


        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-md-2">
                   <img src="@item.ImageUrl" alt="instagram" style="width:250px;height:250px" class="img-fluid rounded-3 insta-image">
                        
                </div>
            }
        </div>

    </div>
</section>
